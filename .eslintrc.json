{
  "root": true,
  "extends": "next",
  "rules": {
    "react/no-unescaped-entities": "off",
    "no-unused-vars": [
      "error", 
      { 
        "vars": "all", 
        "args": "after-used", 
        "ignoreRestSiblings": false
        // "argsIgnorePattern": "^_",
        // "varsIgnorePattern": "^_"
      }
    ],
    "react/display-name": 0
  }
}
